@charset "UTF-8";
.view-header {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 422rpx;
  background: #ff9900;
  color: #231815;
  padding: 0 5px;
}

.view-footer {
  padding-bottom: 30px;
  padding-top: 25px;
}

.header-section,
.header-section__self {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-align-self: center;
          align-self: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  width: 33.33%;
  box-sizing: border-box;
  padding-bottom: 18px;
  overflow: hidden;
}

.header-section__self {
  width: 100%;
}

.header_hd {
  padding-top: 44px;
  position: relative;
}

.header_hd::after {
  content: "";
  display: table;
  clear: both;
}

.header_hd .boxF,
.header_hd .boxS,
.header_hd .boxT,
.header_hd .overlay {
  width: 68px;
  height: 80px;
  overflow: hidden;
  box-shadow: 0rpx 20rpx 20rpx #f87300;
}

.header_hd .boxF,
.header_hd .boxS {
  visibility: hidden;
}

.header_hd .boxF {
  transform: rotate(120deg);
  -webkit-transform: rotate(120deg);
}

.header_hd .boxS {
  transform: rotate(-60deg);
  -webkit-transform: rotate(-60deg);
}

.header_hd .boxT {
  transform: rotate(-60deg);
  background: no-repeat 50% center;
  background-size: 125% auto;
  -webkit-transform: rotate(-60deg);
  visibility: visible;
}

.header_hd .avatar {
  background-color: #ccc;
  background-size: 100% 100%;
  position: relative;
}

.header_hd .overlay {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.header_hd .user {
  max-width: 160px;
}

.header-section:nth-child(2n) .header_hd,
.header-section__self .header_hd {
  padding-top: 28px;
  padding-bottom: 8px;
}

.header-section:nth-child(2n) .boxF,
.header-section:nth-child(2n) .boxS,
.header-section:nth-child(2n) .boxT,
.header-section__self .boxF,
.header-section__self .boxS,
.header-section__self .boxT {
  width: 78px;
  height: 90px;
  overflow: hidden;
  box-shadow: 0rpx 20rpx 20rpx #f87300;
}

.header-section:nth-child(2n) .overlay,
.header-section__self .overlay {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  width: 78px;
  height: 90px;
}

/*评奖*/
.ranking {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
  width: 40px;
  height: 40px;
}

.ranking_1 {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  bottom: -10px;
}

.ranking_0 {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  bottom: -16px;
}

.ranking_2 {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  bottom: -16px;
}

/*End 评奖*/
.header_bd {
  line-height: 2;
  text-align: center;
  overflow: hidden;
  width: 100%;
}

.header_bd .user {
  font-weight: 600;
  box-sizing: border-box;
  padding: 5px;
  width: 100%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.header_bd .counts {
  font-size: .999em;
}

.header_bd .line {
  height: 8rpx;
  width: 26rpx;
  background: #231815;
  margin: 0 auto;
}

/*End .view-header*/
.view-content {
  padding: 0 15px 0 20px;
  color: #231815;
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
}

.view-content .content_logo {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  margin: 20px 0;
}

.view-content .content_logo image {
  width: 157px;
  height: 30px;
}

.content_lists {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 10px 0;
}

.content_lists .lists_hd {
  width: 40rpx;
}

.content_lists .lists_bd {
  font-weight: 500;
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
}

.content_lists .lists_bd image {
  vertical-align: middle;
  margin: 0 15px;
  width: 40px;
  min-width: 40px;
  height: 40px;
  border-radius: 50%;
  border: 1rpx solid #ccc;
  background-color: #c7caca;
}

.content_lists .lists_ft {
  font-weight: 600;
}

.share_bar {
  margin: 15px;
}

.share_bar button {
  background: #ff9900;
}

.adds_bar {
  width: 690rpx;
  height: 200rpx;
  margin: 30rpx auto 0;
}

.adds_bar image {
  width: 100%;
  height: 100%;
}

.button-hover_act {
  background: #a05b05;
}
